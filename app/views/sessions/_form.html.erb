<form action='<%= session_url %>' method='post'>
  <%= auth_token %>
  
  <label for='user_username'>Username: </label>
  <input
    type='text'
    id='user_username'
    name='user[username]'
    value='<%= @user.username %>'>
  
  <label for='user_password'>Password: </label>
  <input
    type='password'
    id='user_password'
    name='user[password]'>
    
  <input type='submit' value='Submit!'>
  
</form>